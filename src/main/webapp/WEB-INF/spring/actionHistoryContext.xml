<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="actionHistoryDao"
			class="com.ktds.actionhistory.dao.ActionHistoryDaoImplForMysql">
			<property name="sqlSessionTemplate" ref="mysqlSessionTemplate" />
	</bean>
	
	<bean id="actionHistoryService"
			class="com.ktds.actionhistory.service.ActionHistoryServiceImpl">
		<property name="actionHistoryDao" ref="actionHistoryDao" />
	</bean>
	

</beans>
